{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020
  },
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "eqeqeq": "error",
    "prettier/prettier": [
      "error",
      { "printWidth": 100, "singleQuote": true, "trailingComma": "all" }
    ]
  }
}